{
  "name": "pbkdf2-compat",
  "version": "2.0.1",
  "description": "This library provides the functionality of PBKDF2 with the ability to use any supported hashing algorithm returned from crypto.getHashes()",
  "main": "./index.js",
  "keywords": [
    "pbkdf2",
    "kdf",
    "salt",
    "hash"
  ],
  "scripts": {
    "coverage": "istanbul cover _mocha -- test/*.js",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "test": "istanbul test mocha -- --reporter list test/*.js",
    "bundle-test": "browserify test/ > test/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crypto-browserify/pbkdf2-compat.git"
  },
  "author": "Daniel Cousens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crypto-browserify/pbkdf2-compat/issues"
  },
  "homepage": "https://github.com/crypto-browserify/pbkdf2-compat",
  "devDependencies": {
    "browserify": "^8.1.1",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0"
  },
  "dependencies": {}
}
